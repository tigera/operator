{
  "index_patterns": [
    "tigera_secure_ee_benchmark_results*"
  ],
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 0,
    "index.lifecycle.name": "tigera_secure_ee_benchmark_results_policy",
    "index.lifecycle.rollover_alias": "tigera_secure_ee_benchmark_results"
  },
  "mappings": {
    "properties": {
      "version": {
        "type": "keyword"
      },
      "kubernetesVersion": {
        "type": "keyword"
      },
      "type": {
        "type": "keyword"
      },
      "node_name": {
        "type": "keyword"
      },
      "timestamp": {
        "type": "date"
      },
      "error": {
        "type": "text"
      },
      "tests": {
        "type": "nested",
        "properties": {
          "section": {
            "type": "keyword"
          },
          "section_desc": {
            "type": "text"
          },
          "test_number": {
            "type": "keyword"
          },
          "test_desc": {
            "type": "text"
          },
          "test_info": {
            "type": "text"
          },
          "status": {
            "type": "text"
          },
          "scored": {
            "type": "boolean"
          }
        }
      }
    }
  }
}
