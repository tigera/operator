        "type": "date"
      },
      "uiSummary": {
        "type": "text"
      },
      "endpoints": {
        "type": "object",
        "enabled": false
      },
      "namespaces": {
        "type": "object",
        "enabled": false
      },
      "services": {
        "type": "object",
        "enabled": false
