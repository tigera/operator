{
  "index_patterns": [
    "tigera_secure_ee_snapshots*"
  ],
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 0,
    "index.lifecycle.name": "tigera_secure_ee_snapshots_policy",
    "index.lifecycle.rollover_alias": "tigera_secure_ee_snapshots"
  },
  "mappings": {
    "properties": {
      "kind": {
        "type": "keyword"
      },
      "apiVersion": {
        "type": "keyword"
      },
      "items": {
        "type": "object",
        "enabled": false
      },
      "metadata": {
        "type": "object"
      },
      "requestStartedTimestamp": {
        "type": "date"
      },
      "requestCompletedTimestamp": {
        "type": "date"
      }
    }
  }
}
