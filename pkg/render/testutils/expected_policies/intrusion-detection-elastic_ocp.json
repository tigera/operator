{
  "apiVersion": "projectcalico.org/v3",
  "kind": "NetworkPolicy",
  "metadata": {
    "name": "calico-system.intrusion-detection-elastic",
    "namespace": "tigera-intrusion-detection"
  },
  "spec": {
    "order": 1,
    "tier": "calico-system",
    "selector": "job-name == 'intrusion-detection-es-job-installer'",
    "types": [
      "Egress"
    ],
    "egress": [
      {
        "action": "Allow",
        "protocol": "UDP",
        "destination": {
          "namespaceSelector": "projectcalico.org/name == 'openshift-dns'",
          "selector": "dns.operator.openshift.io/daemonset-dns == 'default'",
          "ports": [
            5353
          ]
        }
      },
      {
        "action": "Allow",
        "protocol": "TCP",
        "destination": {
          "namespaceSelector": "projectcalico.org/name == 'openshift-dns'",
          "selector": "dns.operator.openshift.io/daemonset-dns == 'default'",
          "ports": [
            5353
          ]
        }
      },
      {
        "action": "Allow",
        "protocol": "TCP",
        "destination": {
          "selector": "k8s-app == 'tigera-secure-es-gateway'",
          "namespaceSelector": "projectcalico.org/name == 'tigera-elasticsearch'",
          "ports": [
            5554
          ]
        }
      }
    ]
  }
}
