{
  "apiVersion": "projectcalico.org/v3",
  "kind": "NetworkPolicy",
  "metadata": {
    "name": "calico-system.tigera-policy-recommendation",
    "namespace": "calico-system"
  },
  "spec": {
    "order": 1,
    "tier": "calico-system",
    "selector": "k8s-app == 'tigera-policy-recommendation'",
    "types": [
      "Egress"
    ],
    "ingress": [],
    "egress": [
      {
        "action": "Allow",
        "protocol": "TCP",
        "destination": {
          "services": {
            "name": "kubernetes",
            "namespace": "default"
          }
        }
      },
      {
        "action": "Allow",
        "protocol": "TCP",
        "destination": {
          "namespaceSelector": "projectcalico.org/name == 'calico-system'",
          "selector": "k8s-app == 'calico-manager'",
          "ports": [
            9443
          ]
        }
      },
      {
        "action": "Allow",
        "protocol": "TCP",
        "source": {
        },
        "destination": {
          "selector": "k8s-app == 'tigera-linseed'",
          "namespaceSelector": "projectcalico.org/name == 'tigera-elasticsearch'",
          "ports": [
            8444
          ]
        }
      },
      {
        "action": "Allow",
        "protocol": "UDP",
        "destination": {
          "namespaceSelector": "projectcalico.org/name == 'kube-system'",
          "selector": "k8s-app == 'kube-dns'",
          "ports": [
            53
          ]
        }
      }
    ]
  }
}
